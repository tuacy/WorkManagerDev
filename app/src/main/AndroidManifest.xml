<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tuacy.workmanagerdev">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".inputoutput.InputOutputWorkerActivity" />
        <activity android:name=".periodic.PeriodicWorkerActivity" />
        <activity android:name=".constraints.ConstraintsWorkerActivity" />
        <activity android:name=".cancel.CancelWorkerActivity" />
        <activity android:name=".then.OrderWorkerActivity" />
        <activity android:name=".combine.CombineWorkerActivity" />
        <activity android:name=".taskchainstream.TaskCharinStreamActivity" />
        <activity android:name=".unique.UniqueWorkerActivity" />
    </application>

</manifest>